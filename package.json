  "license": "MIT",
  "nativescript": {
    "id": "com.main.company1-poc-mobile",
    "templateVersion": "v2",
    "tns-android": {
      "version": "6.3.0"
    },
    "tns-ios": {
      "version": "6.3.0"
    }
  },
  "scripts": {
    "fun": "ng run poc-mobile:tns-preview && tns debug ios",
    "start.poc-mobile.ios": "cd apps/poc-mobile && tns run ios --emulator --",
    "start.poc-mobile.android": "cd apps/poc-mobile && tns debug android --emulator",
    "start.poc-mobile.preview": "cd apps/poc-mobile && tns preview",
    "clean.poc-mobile": "cd apps/poc-mobile && npx rimraf -- hooks node_modules platforms package-lock.json && npm i && npx rimraf -- package-lock.json",
    "preview": "cd apps/poc-mobile && tns preview --bundle",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help"
  },
  "private": true,
